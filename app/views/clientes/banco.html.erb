<div class="container"  >

	<ul class="nav nav-tabs">
	  <li role="navigation" class="active"><a href="#ativos">Clientes ativos</a></li>
	  <li role="navigation"><a href="#pendentes">Clientes com pendencias</a></li>
	</ul>

	<div class="panel" id="ativos" name="ativos">
		<table class="table">
			<thead>
				<th>Nome</th>
				<th>Telefone</th>
				<th>Endereço</th>
				<th>CEP</th>
				<th>Valor</th>
				<th>Vencimento</th>
			</thead>
			<tbody>
				<% @ca.each do |p| %>
				<tr>
					<td><%= p.nome %></td>
					<td><%= p.telefone %></td>
					<td><%= p.endereco %></td>
					<td><%= p.cep %></td>
					<td><%= p.valor %></td>
					<td><%= p.vencimento %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>

	<div class="panel" id="pendentes" name="pendentes">
		<table class="table">
			<thead>
				<th>Nome</th>
				<th>Telefone</th>
				<th>Endereço</th>
				<th>CEP</th>
				<th>Valor</th>
				<th>Vencimento</th>
			</thead>
			<tbody>
				<% @ci.each do |p| %>
				<tr>
					<td><%= p.nome %></td>
					<td><%= p.telefone %></td>
					<td><%= p.endereco %></td>
					<td><%= p.cep %></td>
					<td><%= p.valor %></td>
					<td><%= p.vencimento %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>

</div>