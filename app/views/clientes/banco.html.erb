<ul class="nav nav-tabs" role="tablist">
	<li role="navigation" class="active"><a href="#ativos" aria-controls="ativos" role="tab" data-toggle="tab">Clientes ativos</a></li>
	<li role="navigation"><a href="#pendentes" aria-controls="pendentes" role="tab" data-toggle="tab">Clientes com pendencias</a></li>
</ul>

<div class="tab-content"  >
	<div role="tabpanel" class="tab-pane active" id="ativos">
		<table class="table">
			<thead>
				<th>Nome</th>
				<th>Telefone</th>
				<th>Endereço</th>
				<th>CEP</th>
				<th>Valor</th>
				<th>Vencimento</th>
			</thead>
			<tbody>
				<% @ca.each do |p| %>
				<tr>
					<td><%= p.nome %></td>
					<td><%= p.telefone %></td>
					<td><%= p.endereco %></td>
					<td><%= p.cep %></td>
					<td><%= p.valor %></td>
					<td><%= p.vencimento %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>

	<div role="tabpanel" class="tab-pane" id="pendentes">
		<table class="table">
			<thead>
				<th>Nome</th>
				<th>Telefone</th>
				<th>Endereço</th>
				<th>CEP</th>
				<th>Valor</th>
				<th>Vencimento</th>
			</thead>
			<tbody>
				<% @ci.each do |p| %>
				<tr>
					<td><%= p.nome %></td>
					<td><%= p.telefone %></td>
					<td><%= p.endereco %></td>
					<td><%= p.cep %></td>
					<td><%= p.valor %></td>
					<td><%= p.vencimento %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>

</div>